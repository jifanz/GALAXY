{
	"model": {
		"architecture": "resnet18",
		"target_classes": 10
	},
	"train_parameters": {
		"lr": 0.01,
		"batch_size": 20,
		"n_epoch": 300,
		"max_accuracy": 0.99,
		"isreset": true,
		"islogs":  true,
		"isverbose":  true,
		"logs_location": "./logs.txt"
	},

	"active_learning":{
		"strategy": "glister",
		"budget": 5000,
		"rounds": 15,
		"initial_points":5000,
		
		"strategy_args":{	
			"batch_size" : 20, 
			"lr":0.01
		}
	},
	"dataset":{
		"name":"cifar10"
	}
}